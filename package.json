{
	"name": "chetcorcos.com",
	"version": "1.0.0",
	"description": "boilerplate for web applications using typescript and webpack",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --content-base dist",
		"build": "rm -rf dist && webpack -p && npm run setup200",
		"setup200": "cd dist && cp index.html 200.html",
		"deploy": "cd dist && rm -f *.map && surge . chetcorcos.surge.sh",
		"release": "npm run build && npm run deploy"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"fuzzy": "^0.1.3",
		"glamor": "^2.20.24",
		"lodash": "^4.17.4",
		"moment": "^2.18.1",
		"react": "^15.4.2",
		"react-dom": "^15.4.2",
		"react-router-dom": "^4.2.2",
		"reactive-magic": "^1.9.0"
	},
	"devDependencies": {
		"@types/history": "^4.6.0",
		"@types/lodash": "^4.14.74",
		"@types/react": "^15.0.13",
		"@types/react-dom": "^0.14.23",
		"@types/webpack": "^3.0.10",
		"babel": "^6.23.0",
		"babel-core": "^6.23.1",
		"babel-loader": "^6.3.2",
		"babel-preset-es2015": "^6.22.0",
		"babel-preset-react": "^6.23.0",
		"babel-preset-stage-0": "^6.22.0",
		"file-loader": "^0.11.2",
		"html-webpack-plugin": "^2.28.0",
		"surge": "^0.19.0",
		"ts-loader": "^2.0.1",
		"ts-node": "^3.3.0",
		"tslint": "^5.7.0",
		"tslint-react": "^3.2.0",
		"typescript": "^2.2.1",
		"webpack": "^2.2.1",
		"webpack-dev-server": "^2.4.1"
	}
}

{
	"name": "chetcorcos.com",
	"version": "1.0.0",
	"description": "boilerplate for web applications using typescript and webpack",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --content-base dist",
		"build": "rm -rf dist && webpack -p && npm run setup200",
		"setup200": "cd dist && cp index.html 200.html",
		"deploy": "cd dist && rm -f *.map && surge . chetcorcos.surge.sh",
		"release": "npm run build && npm run deploy"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"fuzzy": "^0.1.3",
		"glamor": "^2.20.40",
		"lodash": "^4.17.4",
		"moment": "^2.18.1",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-router-dom": "^4.2.2",
		"reactive-magic": "^2.1.0"
	},
	"devDependencies": {
		"@types/history": "^4.6.0",
		"@types/lodash": "^4.14.77",
		"@types/react": "^16.0.9",
		"@types/react-dom": "^16.0.0",
		"@types/webpack": "^3.0.13",
		"babel": "^6.23.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"file-loader": "^1.1.4",
		"html-webpack-plugin": "^2.30.1",
		"surge": "^0.19.0",
		"ts-loader": "^2.3.7",
		"ts-node": "^3.3.0",
		"typescript": "^2.5.3",
		"webpack": "^3.6.0",
		"webpack-dev-server": "^2.9.1"
	}
}
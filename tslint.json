{
	"extends": [
		"tslint-react"
	],
	"rules": {
		"indent": [
			true,
			"tabs"
		],
		"jsx-no-lambda": [
			false
		],
		"jsx-no-multiline-js": [
			false
		],
		"jsx-wrap-multiline": [
			false
		],
		"jsx-curly-spacing": [
			false
		],
		"triple-equals": true,
		"no-unused-variable": [
			true
		],
		"semicolon": [
			true,
			"never"
		],
		"curly": true,
		"prefer-const": true,
		"no-debugger": true,
		"no-console": [
			true,
			"log",
			"error",
			"trace"
		]
	}
}